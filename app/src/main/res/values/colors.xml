    <style name="EditTextAppearance" parent="Widget.AppCompat.EditText">
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
    </style>

    <style name="ErrorAlert" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="InfoAlert" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="NavigationTheme" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
    </style>

    <style name="NavigationMenuTextStyle" parent="TextAppearance.AppCompat">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <color name="ucrop_color_toolbar">@color/colorBackground</color>
    <color name="ucrop_color_statusbar">@color/colorBackground</color>
    <color name="ucrop_color_widget_active">@color/colorPrimaryDark</color>
    <color name="ucrop_color_toolbar_widget">@color/colorText</color>
    <color name="ucrop_color_active_controls_color">@color/colorPrimaryDark</color>
    <color name="ucrop_color_inactive_controls_color">@color/colorTextHint</color>
    <color name="ucrop_color_widget_rotate_mid_line">@color/colorPrimaryDark</color>
